{
    "name": "unicon-syntax",
    "displayName": "Unicon Syntax",
    "description": "Syntax highlighting for unicon and icon",
    "author": "Margret Riegert",
	"license": "MIT",
	"version": "0.5.0",
	"publisher": "jafar",
    "engines": {
        "vscode": "^1.82.0"
    },
    "categories": [
        "Programming Languages"
    ],
	"repository": {
		"type": "git",
		"url": "https://github.com/Jafaral/unicon-vscode-syntax.git"
	},
    "contributes": {
        "languages": [{
            "id": "unicon",
            "aliases": ["Unicon", "icn"],
            "extensions": [".icn"],
            "configuration": "./language-configuration-unicon.json"
        },
        {
            "id": "ucode",
            "aliases": ["Ucode"],
            "extensions": [".u", ".u1", ".u2"],
            "configuration": "./language-configuration-ucode.json"
        }],
        "grammars": [{
            "language": "unicon",
            "scopeName": "source.icn",
            "path": "./syntaxes/icn.tmLanguage.json"
        },
        {
            "language": "ucode",
            "scopeName": "source.u",
            "path": "./syntaxes/ucode.tmLanguage.json"    
        }]
    }
}